{
  "name": "readonly",
  "description": "Read-only access to all resources (no writes, no admin)",
  "kv": [
    {
      "path": "*",
      "capabilities": ["read", "list"]
    }
  ],
  "service": [
    {
      "name": "*",
      "capabilities": ["read", "list"]
    }
  ],
  "health": [
    {
      "capabilities": ["read"]
    }
  ],
  "backup": [
    {
      "capabilities": ["deny"]
    }
  ],
  "admin": [
    {
      "capabilities": ["deny"]
    }
  ]
}
